<html>
  <head>
    <script src="/javascripts/angular.min.js"></script>
    <script src="/javascripts/jquery.js"></script>
    <script src="/javascripts/index.js"></script>
    <link rel="stylesheet" href="stylesheets/style.css"> 
  </head>
  <body ng-app="home" ng-controller="collidoscope">
    <div class="container">
      <div class="bar" style="margin-right:0px;background-color:black;"></div>
    </div>
    <div class="time">
      {{ time }}
    </div>
    <span class="c">
      <div ng-click="loop()" class="play"></div>
      <div ng-click="record()" class="record"></div>
    </span>
  </body>
</html>
